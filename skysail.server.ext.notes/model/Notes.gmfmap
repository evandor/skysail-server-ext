    <containmentFeature
        href="Notes.ecore#//Folder/notes"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="Notes.gmftool#//@palette/@tools.0/@tools.0"/>
  </links>
  <links>
    <domainMetaElement
        href="Notes.ecore#//Folder"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <features
          href="Notes.ecore#//Folder/name"/>
    </labelMappings>
    <containmentFeature
        href="Notes.ecore#//Folder/subfolders"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="Notes.gmftool#//@palette/@tools.0/@tools.1"/>
  </links>
  <diagram>
    <diagramCanvas
        href="Notes.gmfgraph#notes"/>
    <domainModel
        href="Notes.ecore#/"/>
    <domainMetaElement
        href="Notes.ecore#//Folder"/>
    <palette
        href="Notes.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
